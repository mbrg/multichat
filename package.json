{
  "name": "multichat",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write \"app/**/*.{ts,tsx,js,jsx,css}\" \"*.{js,json,md}\"",
    "format:check": "prettier --check \"app/**/*.{ts,tsx,js,jsx,css}\" \"*.{js,json,md}\"",
    "vercel:dev": "vercel dev",
    "vercel:build": "vercel build",
    "vercel:deploy": "vercel --prod",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:smoke": "playwright test --project=smoke-chrome",
    "test:e2e:flows": "playwright test flows/",
    "test:e2e:mobile": "playwright test --project=mobile-chrome --project=mobile-safari",
    "test:e2e:performance": "playwright test --project=performance",
    "test:e2e:report": "playwright show-report e2e-results/html-report",
    "test:e2e:install": "playwright install",
    "test:e2e:clean": "rm -rf e2e-results test-results playwright-report",
    "typecheck": "tsc --noEmit",
    "ci": "npm run lint && npm run format:check && npm run typecheck && npm test && npm run build",
    "ci:full": "npm run lint && npm run format:check && npm run typecheck && npm test && npm run test:e2e && npm run build",
    "ci:e2e": "npm run test:e2e:clean && npm run test:e2e"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^1.2.12",
    "@ai-sdk/google": "^1.2.19",
    "@ai-sdk/mistral": "^1.2.8",
    "@ai-sdk/openai": "^1.3.22",
    "@ai-sdk/openai-compatible": "^0.2.14",
    "@auth/core": "^0.34.2",
    "@next/third-parties": "^15.2.0",
    "@tailwindcss/line-clamp": "^0.4.4",
    "@types/react-window": "^1.8.8",
    "@upstash/redis": "^1.31.1",
    "@vercel/analytics": "^1.2.2",
    "@vercel/blob": "^1.1.1",
    "@vercel/functions": "^2.2.2",
    "ai": "^4.3.16",
    "next": "^15.2.0",
    "next-auth": "^4.24.11",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-window": "^1.8.11",
    "redis": "^4.6.7"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@next/eslint-plugin-next": "^15.2.0",
    "@playwright/test": "^1.53.2",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^22.10.8",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@typescript-eslint/eslint-plugin": "^8.34.1",
    "@typescript-eslint/parser": "^8.34.1",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.25.0",
    "eslint-config-next": "^15.2.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.5.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.0.0",
    "jsdom": "^26.1.0",
    "postcss": "^8.5.6",
    "prettier": "^3.5.3",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.30.1",
    "vercel": "^44.2.12",
    "vitest": "^3.2.4"
  },
  "overrides": {
    "cookie": "^0.7.2",
    "path-to-regexp": "^8.2.0",
    "esbuild": "^0.25.5",
    "undici": "^6.21.3"
  }
}
